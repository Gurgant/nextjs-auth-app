{
  "ignorePatterns": [
    {
      "pattern": "^http://localhost"
    },
    {
      "pattern": "^https://localhost"
    }
  ],
  "timeout": "20s",
  "retryOn429": true,
  "retryCount": 3,
  "fallbackTimeout": "30s",
  "aliveStatusCodes": [200, 206, 299, 301, 302, 303, 304, 307, 308],
  "httpHeaders": [
    {
      "urls": ["https://github.com"],
      "headers": {
        "User-Agent": "Documentation Link Checker Bot"
      }
    }
  ]
}